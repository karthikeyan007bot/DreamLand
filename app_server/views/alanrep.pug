                       .fanta-options.float-end 
                         label(for=`annex-fileinp-${annex._id}`).mx-1 
                            span(class="material-symbols-outlined")
                              | add_photo_alternate
                            input(name="fantaimage", type="file", onchange=`readURL(this,"replyImg${annex._id}")` id=`annex-fileinp-${annex._id}`).d-none    
                         a.mx-1(href='#'  data-bs-toggle='collapse' data-bs-target=`#annex-reply-gif-${annex._id}` aria-expanded='false')
