                      .modal-dream-info-counts           
           .annex-footer.d-flex.justify-content-between.mt-2
             .annex-footer-comment 
               button.btn(type='button' tabindex='0' data-bs-toggle="modal"  data-bs-target=`#annex_annex_${annex._id}`)
                 i.fa-regular.fa-comment
               a(href=`/${annex.fminiRefId}/${annex._id}/nestedReplies`) 122 
                 span.lapdisp(style='font-size:15px') replies
             .modal.fade(data-bs-backdrop="static" tabindex='-1' aria-labelledby='fantamodalLabel1' aria-hidden='true' id=`annex_annex_${annex._id}`)
                 .modal-dialog.modal-fullscreen-sm-down
                   .modal-content
                     .modal-header
                          beutton.btn-close(type='button' data-bs-dismiss='modal' aria-label='Close') 
